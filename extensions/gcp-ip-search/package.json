{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gcp-ip-search",
  "title": "Google Cloud Platform IP Search",
  "description": "Search for IP addresses across all your GCP projects and quickly navigate to the GCP console",
  "icon": "icon.png",
  "author": "pin-yi",
  "categories": [
    "Developer Tools",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "platforms": [
    "macOS"
  ],
  "commands": [
    {
      "name": "search-ip",
      "title": "Search GCP IP Address",
      "description": "Search for an IP address across all GCP projects",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.3",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "20.8.10",
    "@types/react": "^19.2.9",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
