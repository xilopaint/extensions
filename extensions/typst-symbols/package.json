{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "typst-symbols",
  "title": "Typst Math Symbols",
  "description": "Search through a library of typst math symbols and emojis",
  "icon": "icon.png",
  "author": "JeffersonDing",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Typst Symbols",
      "description": "Search through a library of typst math symbols and emojis",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.1",
    "fuse.js": "^7.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
