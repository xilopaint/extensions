{
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": [
    "src/**/*",
    "raycast-env.d.ts"
  ],
  "compilerOptions": {
    "lib": [
      "ES2023"
    ],
    "module": "commonjs",
    "target": "ES2023",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}