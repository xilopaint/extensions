      "name": "ask-t3-chat",
      "title": "Ask T3 Chat",
      "description": "Quickly ask T3 Chat anything",
      "mode": "no-view",
      "arguments": [
        {
          "name": "query",
          "type": "text",
          "required": true,
          "placeholder": "Query"
        },
        {
          "name": "search",
          "type": "dropdown",
          "required": false,
          "placeholder": "Search Grounding",
          "description": "Enable search grounding for models that support it",
